{
    "buildCommand": "cd pages/journal-pwa && npm install && npm run build",
    "outputDirectory": ".",
    "rewrites": [
        {
            "source": "/pages/journal-pwa/(.*)",
            "destination": "/pages/journal-pwa/dist/$1"
        },
        {
            "source": "/pages/journal-pwa",
            "destination": "/pages/journal-pwa/dist/index.html"
        }
    ]
}